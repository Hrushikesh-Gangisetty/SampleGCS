<?xml version="1.0" encoding="utf-8"?>
<!--
    This drawable is used for the splash screen.
    It references @drawable/pavaman_logo, which is expected to be a PNG image file
    located in the res/drawable/ folder.
    Make sure to add your pavaman_logo.png to the res/drawable/ folder.
-->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:drawable="@drawable/pavaman_logo"
        android:gravity="center" />
</layer-list>
